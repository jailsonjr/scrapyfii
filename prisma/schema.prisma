
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]  
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model tickers_info {
  id String @id @default(uuid())
  ticker String @unique
  ticker_type String?
  ticker_name String? @unique
  ticker_admin String?
}